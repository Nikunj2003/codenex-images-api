{
  "version": 2,
  "routes": [
  { "src": "^/$", "dest": "/api" },
  { "src": "^/api/health$", "dest": "/api/health" },
  { "src": "^/api/docs$", "dest": "/api/docs" },
  { "src": "^/api/docs.json$", "dest": "/api/docs" },
  { "src": "^/api/(swagger-ui.*\\.(css|js|map|png))$", "dest": "/api/docs" },
  { "src": "/api/(.*)", "dest": "/api/$1" }
  ]
}
